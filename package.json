  "name": "@purelightpower/jsforce",
  "description": "Salesforce API Library for JavaScript",
  "keywords": [
    "salesforce",
    "salesforce.com",
    "sfdc",
    "force.com",
    "database.com"
  ],
  "homepage": "http://github.com/purelightpower/jsforce",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/purelightpower/jsforce.git"
